collectd:
  FQDNLookup: true
  plugin: true
  plugins:
    defaults: [battery, cpu, entropy, load, memory, swap, load, users]
    syslog:
      LogLevel: info
    network:
      host: 'logstash'
      port: 25826
    mysql:
      host: 'localhost'
      port: '3306'
      user: 'myuser'
      pass: 'mypass'
      name: 'mydb'
      MasterStats: 'false'
    postgresql:
      host: 'localhost'
      port: '5432'
      user: 'myuser'
      pass: 'mypass'
      name: 'mydb'
    df:
      Device:
      MountPoint:
      FSType: 'ext4'
      IgnoreSelected: false
      ReportByDevice:
      ReportReserved:
      ReportInodes:
    ntpd:
      host: localhost
      port: 123
      ReverseLookups: 'false'
    java:
      host: localhost
      port: 17264
      user: ''
      pass: ''
    ethstat:
      interface: 'eth0'
